{
  "name": "GetIPs",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "Get Intranet IP And Internet IP",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "GetIPs",
    "default_popup": "popup.html"
  },
  "permissions": [
      "tabs", "http://*/*"
  ],
  "content_scripts": [
    {
        "matches": ["http://*/*"],
        "js": ["page.js"]
    }
  ]
}
